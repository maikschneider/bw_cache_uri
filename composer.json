{
  "name": "blueways/bw-cache-uri",
  "description": "TYPO3 extension to parse remote content and save to tt_content element",
  "homepage": "https://blueways.de",
  "license": "proprietary",
  "keywords": [
    "TYPO3 CMS"
  ],
  "require": {
    "symfony/http-client": "^4.3",
    "symfony/dom-crawler": "^4.3",
    "symfony/css-selector": "^4.3",
    "typo3/cms-scheduler": "^9.5"
  },
  "require-dev": {
    "typo3/cms-base-distribution": "^9.5",
    "helhum/typo3-console": "^5.7"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "public"
    }
  },
  "config": {
    "bin-dir": "bin",
    "vendor-dir": "vendor",
    "platform": {
      "php": "7.2"
    }
  },
  "autoload": {
    "psr-4": {
      "Blueways\\BwCacheUri\\": "Classes/"
    }
  }
}
